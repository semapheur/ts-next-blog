    "key": "altgr+4",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "$ $"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && ['mdx', 'markdown'].includes(editorLangId)"
  },

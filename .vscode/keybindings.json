[
  {
    "key": "altgr+4",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "$ $"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  },
  {
    "key": "ctrl+1",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "\\mathbf{}"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  },
  {
    "key": "ctrl+shift+1",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "\\boldsymbol{}"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  },
  {
    "key": "ctrl+2",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "\\mathrm{}"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  },
  {
    "key": "ctrl+3",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "\\mathbb{}"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  },
  {
    "key": "ctrl+4",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "\\mathcal{}"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  },
  {
    "key": "ctrl+5",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "<details>\n<summary>Proof</summary>\n\n</details>"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  },
  {
    "key": "ctrl+6",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "```math\n\n```"
    },
    "when": "config.workspaceKeybindings.latex.enabled && editorTextFocus && editorLangId == 'mdx'"
  }
]